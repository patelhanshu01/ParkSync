{
  "name": "smart-parking-backend",
  "version": "1.0.0",
  "description": "Smart Parking System Backend API",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "ts-node-dev --respawn --transpile-only src/main.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "migration:generate": "npm run build && typeorm migration:generate -d dist/config/database.config.js",
    "migration:run": "npm run build && typeorm migration:run -d dist/config/database.config.js",
    "migration:revert": "npm run build && typeorm migration:revert -d dist/config/database.config.js",
    "test": "jest --config jest.config.js --runInBand"
  },
  "keywords": [
    "parking",
    "smart-parking",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "google-auth-library": "^10.5.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "stripe": "^20.1.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.8.10",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.0",
    "@types/jest": "^29.5.5",
    "supertest": "^6.4.3",
    "@types/supertest": "^2.0.12"
  }
}